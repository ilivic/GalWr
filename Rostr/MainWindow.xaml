<Window x:Class="Rostr.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Rostr"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="6*"/>
        </Grid.ColumnDefinitions>
        <StackPanel>
            <Button Content="Show" Click="Button_Click"/>
            <Button Content="Add" Click="Button_Click_1"/>
            <Button Content="Add Content" Click="Button_Click_2"/>
        </StackPanel>
        <Frame x:Name="MainFraimNavigation" Grid.Column="1" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
